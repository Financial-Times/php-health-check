{
    "name" : "ft/healthcheck-bundle",
    "description" : "Symfony bundle for running FT healthchecks",
    "type" : "symfony-bundle",
    "authors" : [{
        "name" : "Ryan.Lee",
        "email" : "rizza@rizza.net"
    },{
        "name": "Alex wilson"   
    }],
    "keywords" : [
        "FT",
        "Healthcheck",
        "Symfony"
    ],
    "license" : [
        "MIT"
    ],
    "require" : {
        "php": ">=7.1",
        "symfony/config": "^2.8",
        "sensio/framework-extra-bundle": "^3.0",
        "symfony/dependency-injection": "^2.8",
        "symfony/http-foundation": "^2.8",
        "symfony/http-kernel": "^2.8",
        "symfony/security": "^2.8",
        "symfony/yaml": "^2.8",
        "monolog/monolog": "^2.0|^1.17",
        "psr/cache": "^1.0",
        "ocramius/proxy-manager": "^1.0|^2.0"
    },
    "autoload" : {
        "psr-4": { 
            "FT\\": "src/"
        }
    },
    "autoload-dev" : {
        "psr-4": { 
            "FT\\Tests\\": "tests/"
        }
    },
    "require-dev": {
        "mockery/mockery": "^1.3",
        "phpunit/phpunit": "^7"
    }
}
